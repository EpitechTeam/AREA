<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/content_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/page_my_waves_ek5"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp">

        <RelativeLayout
            android:id="@+id/top_ek2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp">

            <View
                android:id="@+id/background_ek16"
                android:layout_width="wrap_content"
                android:layout_height="70dp"
                android:layout_marginStart="0dp"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dp"
                android:background="@drawable/background_ek16_shape" />

            <ImageButton
                android:id="@+id/add_ek2"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_centerVertical="true"
                android:layout_marginStart="280dp"
                android:layout_marginLeft="280dp"
                android:layout_marginTop="23dp"
                android:background="#FFFFFF"
                android:contentDescription="add_ek2"
                android:onClick="addWaves"
                android:scaleType="fitCenter"
                android:src="@drawable/add_ek2" />

            <TextView
                android:id="@+id/my_waves_ek7"
                android:layout_width="125dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:layout_gravity="center_horizontal|center_vertical"
                android:layout_marginLeft="116dp"
                android:layout_marginTop="20dp"
                android:fontFamily="Roboto"
                android:gravity="center"
                android:text="My Waves"
                android:textColor="#1c1c1c"
                android:textSize="26sp" />


        </RelativeLayout>

        <ScrollView
            android:layout_marginTop="60dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/background_ek16_shape"
            >

            <RelativeLayout
                android:id="@+id/item_ek3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="40dp"
                android:layout_marginTop="90dp">


                <View
                    android:id="@+id/background_ek15"
                    android:layout_width="330dp"
                    android:layout_height="300dp"
                    android:layout_marginStart="0dp"
                    android:layout_marginLeft="0dp"
                    android:layout_marginTop="0dp"
                    android:background="@drawable/background_ek15_shape" />


                <RelativeLayout
                    android:id="@+id/bottom_ek6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="240dp">

                    <View
                        android:id="@+id/bottom_ek7"
                        android:layout_width="330dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="0dp"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="0dp"
                        android:alpha="0.2"
                        android:background="@drawable/bottom_ek7_shape" />


                    <RelativeLayout
                        android:id="@+id/works_ek3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="170dp"
                        android:layout_marginTop="20dp">


                        <RelativeLayout
                            android:id="@+id/onedrive_ek3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="98dp"
                            android:layout_marginTop="0dp">


                            <RelativeLayout
                                android:id="@+id/group_2_ek3"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="0dp"
                                android:layout_marginTop="0dp">


                                <RelativeLayout
                                    android:id="@+id/group_1_ek3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="0dp"
                                    android:layout_marginTop="0dp">

                                    <ImageView
                                        android:id="@+id/path_2_ek3"
                                        android:layout_width="24.558532714844dp"
                                        android:layout_height="16.869018554688dp"
                                        android:layout_marginStart="0dp"
                                        android:layout_marginLeft="0dp"
                                        android:layout_marginTop="0dp"
                                        android:contentDescription="path_2_ek3"
                                        android:src="@drawable/path_2_ek3" />

                                </RelativeLayout>

                            </RelativeLayout>


                            <RelativeLayout
                                android:id="@+id/group_4_ek3"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="7dp"
                                android:layout_marginTop="4dp">


                                <RelativeLayout
                                    android:id="@+id/group_3_ek3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="0dp"
                                    android:layout_marginTop="0dp">

                                    <ImageView
                                        android:id="@+id/path_3_ek3"
                                        android:layout_width="25.294311523438dp"
                                        android:layout_height="15.926025390625dp"
                                        android:layout_marginStart="0dp"
                                        android:layout_marginLeft="0dp"
                                        android:layout_marginTop="0dp"
                                        android:contentDescription="path_3_ek3"
                                        android:src="@drawable/path_3_ek3" />

                                </RelativeLayout>

                            </RelativeLayout>

                        </RelativeLayout>

                        <TextView
                            android:id="@+id/works_with_ek3"
                            android:layout_width="88dp"
                            android:layout_height="23dp"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="0dp"
                            android:fontFamily="Roboto"
                            android:text="works with"
                            android:textColor="#ffffff"
                            android:textSize="17sp" />

                    </RelativeLayout>

                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/facebook_app_logo_ek1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="20dp">

                    <ImageView
                        android:id="@+id/path_1_ek1"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginStart="0dp"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="0dp"
                        android:contentDescription="path_1_ek1"
                        android:src="@drawable/path_1_ek1" />

                </RelativeLayout>

                <TextView
                    android:id="@+id/__p__save_photos_you_are_tagged_in_to_one_drive_ek3"
                    android:layout_width="282dp"
                    android:layout_height="127dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="82dp"
                    android:fontFamily="Roboto"
                    android:text="Save photos you are tagged in to One Drive"
                    android:textColor="#ffffff"
                    android:textSize="28sp" />


            </RelativeLayout>

        </ScrollView>

    </RelativeLayout>

</RelativeLayout>

